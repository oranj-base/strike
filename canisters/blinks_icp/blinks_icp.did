type BlinkRegistry = record {
  website_url : opt text;
  tags : vec Tag;
  action_url : text;
  created_at : nat64;
  blink_url : opt text;
};
type Tag = variant { Submitted };
service : {
  add_registry : (BlinkRegistry) -> ();
  get_heap_data : () -> (blob) query;
  set_heap_data : (blob) -> ();
  stable_get : (nat64) -> (opt BlinkRegistry) query;
  stable_insert : (nat64, BlinkRegistry) -> ();
}
