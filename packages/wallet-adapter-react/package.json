{
  "name": "@oranjlabs/icp-wallet-adapter-react",
  "author": "Edward Lee <edward@oranj.co>",
  "version": "0.0.1",
  "sideEffects": true,
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./hooks": {
      "import": "./dist/hooks/index.js",
      "require": "./dist/hooks/index.cjs",
      "types": "./dist/hooks/index.d.ts"
    },
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./index.css": "./dist/index.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup-node",
    "dev": "tsup-node --watch"
  },
  "dependencies": {
    "@xstate/react": "^4.1.1",
    "neverthrow": "^5.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "tsup": "^8.2.0",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "@oranjlabs/icp-wallet-adapter": "*",
    "react": "*"
  }
}
