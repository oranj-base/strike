{
  "canisters": {
    "strike_backend": {
      "candid": "canisters/strike_backend/strike_backend.did",
      "package": "strike_backend",
      "type": "rust",
      "declarations": {
        "node_compatibility": true
      }
    },
    "hello": {
      "candid": "canisters/hello/hello.did",
      "package": "hello",
      "type": "rust",
      "declarations": {
        "node_compatibility": true
      }
    },
    "strike_frontend": {
      "dependencies": ["strike_backend"],
      "source": ["apps/site/.next"],
      "frontend": {
        "entrypoint": "apps/site/.next/server/app/index.html"
      },
      "type": "assets",
      "workspace": "@oranjbase/strike-site"
    },
    "internet_identity": {
      "type": "custom",
      "candid": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity.did",
      "frontend": {},
      "wasm": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity_dev.wasm.gz",
      "shrink": false,
      "remote": {
        "candid": "internet_identity.did",
        "id": {
          "ic": "rdmx6-jaaaa-aaaaa-aaadq-cai"
        }
      }
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "metadata": [
    {
      "name": "candid:service"
    }
  ],
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943",
      "type": "ephemeral"
    },
    "staging": {
      "providers": ["https://icp0.io"],
      "type": "persistent"
    }
  },
  "output_env_file": ".env",
  "version": 1
}
